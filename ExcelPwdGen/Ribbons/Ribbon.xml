	<ribbon>
		<tabs>
			<tab idMso="TabAddIns">
				<group id="PSMainGroup" label="ExcelPwdGen">
					<button id="SaveBtn" onAction="SaveBtn_Click"  label="保存" showImage="true" size="normal" imageMso="FileSave" supertip="ワークブックを保存または上書き保存します" />
					<button id="SaveAsBtn" onAction="SaveAsBtn_Click" label="別名で保存"  showImage="true" size="normal" imageMso="FileSaveAs" supertip="ワークブックを別名で保存します" />
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>